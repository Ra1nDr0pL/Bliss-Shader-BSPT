#version 120
//#extension GL_ARB_shader_texture_lod : disable

#include "/lib/settings.glsl"

flat in int isWater;
in vec2 texcoord;
in float overdrawCull;

uniform sampler2D tex;

//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////
//////////////////////////////VOID MAIN//////////////////////////////

void main() {

	if(isWater > 0){   
		discard;
		return;
	}

	if(overdrawCull < 1.0){   
		discard;
		return;
	}

	gl_FragData[0] = texture2D(tex, texcoord.xy);
}
